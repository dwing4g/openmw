                  <property name="toolTip">
                   <string>&lt;html&gt;&lt;head/&gt;&lt;body&gt;&lt;p&gt;渲染带鞘的武器（带箭筒和刀鞘），需要额外的mod资源。&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;</string>
                  </property>
                  <property name="text">
                   <string>武器护套</string>
                  </property>
                 </widget>
                </item>
               </layout>
              </item>
              <item row="3" column="0">
                <layout class="QVBoxLayout" name="shieldSheathingLayout">
